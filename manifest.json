{
"name": "ReverseProxy",
"id": "reverseProxy",
"description": {
"en": "Simple Reverse Proxy for Yunohost",
"fr": "Simple Reverse Proxy pour Yunohost"
},
"developer": {
"name": "cabbes",
"email": "shornero@gmail.com",
"url": "https://github.com/cabbes/ReverseProxy_ynh"
